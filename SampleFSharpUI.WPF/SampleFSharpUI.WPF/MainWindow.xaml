<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Sample F# UI - WPF" Height="300" Width="400">
    <Grid>
        <TextBox 
            x:Name="text1"
            FontSize="30"
            HorizontalAlignment="Left" Height="46" Margin="10,10,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="238"/>
        <TextBox 
            x:Name="text2"
        	FontSize="30"
        	HorizontalAlignment="Left" Height="46" Margin="10,61,0,0" TextWrapping="Wrap" Text="20" VerticalAlignment="Top" Width="238"/>
        <TextBlock
            x:Name="text3"
            FontSize="30"
            HorizontalAlignment="Left" Margin="10,159,0,0" TextWrapping="Wrap" Text="Ans." VerticalAlignment="Top" Width="238"/>
        <Button 
            x:Name="button1"
            FontSize="30"
            Content="Button" HorizontalAlignment="Left" Margin="10,112,0,0" VerticalAlignment="Top" Width="238"/>
    </Grid>

</Window>